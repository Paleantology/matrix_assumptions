:q#!/bin/bash
#PBS -q workq
#PBS -l nodes=1:ppn=20
#PBS -l walltime=48:00:00
#PBS -N tri_PT 
#PBS -o tri_part.out
#PBS -j oe
#PBS -A loni_antfbe
#PBS -m abe
#PBS -M courtney.grigsby@selu.edu

#path to data not scripts(pwd)
datapath=/work/tyler333/matrix_assumptions/data/binary/
cd $datapath


for file in *.nex
do
echo $file
if [ -f ../trinary/1/"$file" ];
then echo $file
cp $file bin
cp ../trinary/1/"$file" ../trinary/trin
~/rb /work/tyler333/matrix_assumptions/scripts/trinary/tri_analytical_partitioned/mcmc_simple.Rev
fi
mv /work/tyler333/matrix_assumptions/data/trinary/output/part_out/simple.log /work/tyler333/matrix_assumptions/output/trinary/partitioned/$file.log
mv /work/tyler333/matrix_assumptions/data/trinary/output/part_out/simple.trees /work/tyler333/matrix_assumptions/output/trinary/partitioned/$file.trees
done
